{
  "name": "@vse-bude/backend",
  "version": "0.0.1",
  "private": true,
  "main": "./src/server.ts",
  "engines": {
    "npm": ">=8.0.0 <9.0.0",
    "node": ">=16.0.0 <17.0.0"
  },
  "prettier": "@vse-bude/prettier-config",
  "scripts": {
    "start": "nodemon -r ts-node --files ./src/server.ts",
    "build": "tsc && tsc-alias",
    "serve": "node build/server.js",
    "db:generate": "prisma generate",
    "db:migrate:create": "prisma migrate dev --create-only --name",
    "db:migrate": "prisma migrate dev",
    "db:seed": "ts-node --transpile-only --files ./prisma/seed/update",
    "db:seed:delete": "ts-node --transpile-only --files ./prisma/seed/delete",
    "prettier:check": "prettier --check \"*/**/*.{ts,tsx}\" --loglevel=warn",
    "prettier:fix": "prettier --write \"*/**/*.{ts,tsx}\" --loglevel=warn",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.145.0",
    "@prisma/client": "^4.1.1",
    "@types/jsonwebtoken": "^8.5.8",
    "@vse-bude/shared": "file:../shared/build",
    "aws-sdk": "^2.1194.0",
    "cors": "^2.8.5",
    "express": "^4.18.1",
    "i18next": "^21.9.1",
    "i18next-fs-backend": "^1.1.5",
    "i18next-http-middleware": "^3.2.1",
    "ioredis": "^5.2.3",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.5-lts.1",
    "node-fetch": "^2.6.7",
    "sib-api-v3-typescript": "^2.2.2",
    "swagger-jsdoc": "^6.2.5",
    "swagger-ui-express": "^4.5.0",
    "twilio": "^3.80.0",
    "winston": "^3.8.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.4.0",
    "@mermaid-js/mermaid-cli": "^9.1.4",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/node": "^18.7.1",
    "@vse-bude/prettier-config": "file:../prettier-config",
    "nodemon": "2.0.19",
    "prettier": "^2.7.1",
    "prisma": "^4.1.1",
    "prisma-erd-generator": "^1.0.2",
    "ts-node": "^10.1.0",
    "tsc-alias": "^1.7.0",
    "tsconfig-paths": "^4.0.0"
  }
}
